			<y-m>0</y-m>
			<z-m>-2.2</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>
  <animation>
    <type>select</type>
    <object-name>1x Hummer</object-name>      
    <condition>
      <and>
        <equals>
          <property>sim/weight[0]/selected</property>
          <value>1x Hummer</value>
        </equals>
        <equals>
          <property>controls/armament/station[0]/release</property>
          <value>false</value>
        </equals>
