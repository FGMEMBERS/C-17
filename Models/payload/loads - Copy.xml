			<z-m>0</z-m>
			<pitch-deg>0.0</pitch-deg>
		</offsets>
	</model>	



<!-- Cargobay -->

	<model>
		<name>1x Hummer</name>
		<path>Aircraft/fgukpayloads/hummvee/Hummer.xml</path>
