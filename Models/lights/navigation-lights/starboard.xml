
<PropertyList>

 <path>starboard.ac</path>
 
   <effect>
   <inherits-from>Effects/model-transparent</inherits-from>
   <condition>
    <property>/sim/rendering/rembrandt/enabled</property>
   </condition>
  <object-name>aura</object-name>
  </effect>
  
 <animation>
  <type>select</type>
  <object-name>aura</object-name>
  <object-name>beam</object-name>
  <condition>
    <property>sim/multiplay/generic/int[4]</property>
  </condition>
 </animation>
 
  <animation>
    <type>light</type>
    <light-type>point</light-type>
    <object-name>beam</object-name>
	<nopreview/>
    <position>
      <x> -0.2774 </x>
      <y> -0.034 </y>
      <z> 0 </z>
    </position>
    <ambient>
      <r>  0.2 </r>
      <g>  0.4 </g>
      <b>  0.2 </b>
      <a>  0.8 </a>
    </ambient>
    <diffuse>
      <r>  0.3 </r>
      <g>  0.9 </g>
      <b>  0.3 </b>
      <a>  0.9 </a>
    </diffuse>
    <specular>
      <r>  0.75 </r>
      <g>  1.0 </g>
      <b>  0.75 </b>
      <a>  1.0 </a>
    </specular>
     <dim-factor>
      <property>sim/model/lights/daylight-adjuster</property>
      <factor>-1.1</factor>
	  <offset>1.1</offset>
      <min>0</min>
      <max>1.0</max>
   </dim-factor>
   <attenuation>
     <c>1</c>
     <l>1</l>
     <q>1</q>
   </attenuation>
   <near-m>0.1</near-m>
   <far-m>10</far-m>
 </animation>
 
  <animation>
  <type>dist-scale</type>
  <object-name>aura</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>1</dep>
   </entry>
   <entry>
    <ind>500</ind>
    <dep>10</dep>
   </entry>
   <entry>
    <ind>4000</ind>
    <dep>15</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.2774</x-m>
   <y-m>-0.034</y-m>
   <z-m>0</z-m>
  </center>
 </animation>
 
 <animation>
  <type>flash</type>
  <object-name>aura</object-name>
  <offset>0.1</offset>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-0.5</x>
   <y>0.65</y>
   <z>0.2</z>
  </axis>
  <power>1</power>
 </animation>
 
 <animation>
  <object-name>aura</object-name>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>3000</max-m>
 </animation>
 
 <animation>
  <type>blend</type>
  <object-name>aura</object-name>
  <property>sim/model/lights/daylight-adjuster</property>
  <factor>1.2</factor>
  <min>0.0</min>
  <max>1</max>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>aura</object-name>
  <emission>
    <red>1</red>
    <green>1</green>
    <blue>1</blue>
  </emission>
 </animation>
 
</PropertyList>