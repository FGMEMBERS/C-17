    <animation>
      <type>pick</type>
      <object-name>BARO.knob2</object-name>
      <action>
        <button>0</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property>instrumentation/altimeter/setting-inhg</property>
          <step>0.01</step>
          <min>26.0</min>
          <max>33.0</max>
          <wrap>false</wrap>
        </binding>
      </action>
    </animation>

    <animation>
      <type>pick</type>
      <object-name>BARO.knob2</object-name>
      <action>
        <button>1</button>
        <repeatable>true</repeatable>
