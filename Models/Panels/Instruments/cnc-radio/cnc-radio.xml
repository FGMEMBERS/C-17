<PropertyList>
  <path>cnc-radio.ac</path>

  <!-- Brightness buttons -->
  <animation>
    <type>pick</type>
    <object-name>button.bright.low</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.20</interval-sec>
      <binding>
	<command>property-adjust</command>
	<property>/fdm/jsbsim/instruments/cnc-radio/display/config/brt</property>
	<step>-0.10</step>
	<min>0</min>
	<max>1</max>
	<wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>button.bright.hi</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
