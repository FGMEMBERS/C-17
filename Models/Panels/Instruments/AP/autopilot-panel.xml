<?xml version="1.0"?>

<PropertyList>
  <path>autopilot-panel.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>CPT-COURSE</object-name>
    <property>/instrumentation/nav[0]/radials/selected-deg</property> 
    <factor>-5.0</factor>         
    <center>
      <x-m>0.04169</x-m>
      <y-m>-0.2029</y-m>
      <z-m>0.03627</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation> 

  <animation>
    <type>rotate</type>
    <object-name>HDG-SEL</object-name>
    <property>/autopilot/settings/heading-bug-deg</property> 
    <factor>-5.0</factor>         
    <center>
      <x-m>0.03992</x-m>
      <y-m>-0.04286</y-m>
      <z-m>0.04001</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>BANK</object-name>
    <property>/controls/autoflight/bank-angle-select</property>          
    <factor>-5.8</factor> 
    <offset-deg>240</offset-deg>         
    <center>
      <x-m>0.03992</x-m>
      <y-m>-0.04286</y-m>
      <z-m>0.04001</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>at-arm</object-name>
    <property>/controls/autoflight/autothrottle-arm</property> 
    <factor>-30.0</factor>         
    <center>
      <x-m>0.02</x-m>
      <y-m>-0.17</y-m>
      <z-m>0.04</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>   

  <animation>
