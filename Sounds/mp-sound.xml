<engine>
      <name>engine-start-outside0</name>
      <mode>once</mode>
      <path>Aircraft/C-17/Sounds/rb211-startup.wav</path>
      <condition>
<and>
 
<greater-than>  
   <property>engines/engine[0]/rpm</property>
   <value>0.5</value>
   
