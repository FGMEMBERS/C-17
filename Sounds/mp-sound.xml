  <!-- Engines reverse thrust-->
 <!-- Left side -->
    
  
   <engine>
    <name>left outer engine reverser1</name>
        <path>Aircraft/C-17/Sounds/reverser.wav</path>
        <mode>looped</mode>
   <condition>
     <not>
          <property>sim/current-view/internal</property>
	  
     </not>
   </condition>
   <position>
            <x>15</x>
            <y>15</y>
            <z>-3</z>
        </position>
   <orientation>
	<x>-90.0</x>
    	<y>0</y>
    	<z>0</z>
    	<inner-angle>110.0</inner-angle>
    	<outer-angle>180.0</outer-angle>
    	<outer-gain>0.4</outer-gain>
   </orientation>
  <volume>
            <property>surface-positions/reverser-norm[0]</property>
        </volume>
   <pitch>
    	<property>engines/engine/n2</property>
	<factor>0.05</factor>
	<min>0.0</min>
	<max>1.0</max>
   </pitch>
    <reference-dist>600.0</reference-dist>
        <max-dist>6000.0</max-dist>
  </engine>
  
  
  
  
