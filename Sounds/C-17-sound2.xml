                <value>0</value>
            </equals>
        </condition>
        <position>
            <x>6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>20.0</max-dist>
    </gear1>

<squeal0>
