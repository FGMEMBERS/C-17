        <path>Aircraft/C-17/Sounds/gear.wav</path>
        <property>/gear/gear[0]/position-norm</property>
        <pitch>
            <internal>dt_play</internal>
            <offset>0.7</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>6</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>5.0</reference-dist>
