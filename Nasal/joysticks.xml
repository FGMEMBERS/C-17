<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <button n="6">
    <name>rudder-yaw-up</name>
    <desc>Decrease Rudder deflection power</desc>
    <binding>
      <command>property-adjust</command>
      <property>/fdm/jsbsim/systems/multipliers/rudder-yaw</property>
      <step>-0.01</step>
      <min>0.0050</min>
      <max>10.000</max>
      <wrap>true</wrap>
    </binding>      
  </button>
  <button n="7">
    <name>rudder-yaw-down</name>
    <desc>Increase Rudder deflection power</desc>
    <binding>
      <command>property-adjust</command>
      <property>/fdm/jsbsim/systems/multipliers/rudder-yaw</property>
      <step>0.01</step>
      <min>0.0050</min>
      <max>10.000</max>
      <wrap>true</wrap>
    </binding>      
  </button>
</PropertyList>

