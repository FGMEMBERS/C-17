<?xml version="1.0"?>
<PropertyList>
  <default>
    <menu n="100">				
      <label>Globemaster III</label>
      <enabled type="bool">true</enabled>

      <item>
	<label>Auto-coordination</label>
	<binding>
	  <command>nasal</command>
	  <script>
	    var autocoordination = gui.Dialog.new("sim/gui/dialogs/Autocoordination/dialog", "Dialogs/Autocoordination.xml");
	    autocoordination.open();
	  </script>
	</binding>
      </item>

      
      <item>
	<label>Auto Start</label>
	<binding>
	  <command>nasal</command>
	  <script>
	  autostart.StartUp();</script>
	</binding>
      </item>

      <item>
	<label>Cargobay loading</label>
	<binding>
	  <command>nasal</command>
	  <script>
	    var cargobay = gui.Dialog.new("sim/gui/dialogs/Cargobay/dialog", "Dialogs/Cargobay.xml");
	    cargobay.open();
	  </script>
	</binding>
      </item>

      <item>
	<label>Update Livery</label>
	<binding>
	  <command>nasal</command>
	  <script>
	    var livery = gui.Dialog.new("sim/gui/dialogs/Livery/dialog", "Dialogs/Livery.xml");
	    livery.open();
	  </script>
	</binding>
      </item>
    </menu>		
  </default>	
</PropertyList>
