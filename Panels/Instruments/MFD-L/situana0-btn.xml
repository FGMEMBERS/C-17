<?xml version="1.0"?>
<PropertyList>

 <path>mfd-buttons.ac</path>
 
<animation>
  <type>pick</type>
  <object-name>L1</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>instrumentation/MFD[0]/situana/heading-mag</property>
